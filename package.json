{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "watch": "webpack --watch --mode=development --config ./webpack.dev.config.js",
    "build": "webpack --mode=production --node-env=production --config ./webpack.production.config.js",
    "build:dev": "webpack --mode=development --node-env=dev --config ./webpack.dev.config.js",
    "build:prod": "webpack --mode=production --node-env=production -config ./webpack.production.config.js",
    "serve": "webpack serve",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest_setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:MattFMurphy/React-Boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "transform": "",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.15.8",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@webpack-cli/generators": "^2.1.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^5.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^2.4.5",
    "node-fetch": "^3.1.0",
    "sass": "^1.22.6",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-wordcloud": "^1.2.7"
  }
